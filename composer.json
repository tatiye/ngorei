{
    "name": "tatiye/ngorei",
    "description": "Bangun aplikasi iOS, Android & Desktop berfitur lengkap",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
          "app\\": "app/helpers/",
          "app\\config\\": "app/config/",
          "app\\models\\": "app/models/",
          "public\\": "public/"
        }
    },
    "authors": [
        {
            "name": "tatiye",
            "email": "info@tatiye.net"
        }
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "doctrine/sql-formatter": "^1.1",
        "dompdf/dompdf": "^2.0",
        "phpoffice/phpspreadsheet": "^1.29",
        "tatiye/terminal": "dev-main"
    },
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }

}
